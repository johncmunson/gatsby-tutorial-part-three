{
    "name": "gatsby-starter-hello-world",
    "description": "Gatsby hello world starter",
    "license": "MIT",
    "scripts": {
        "develop": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "prettier": "prettier --write \"src/**/*.{js,json,css,md}\"",
        "precommit": "lint-staged"
    },
    "dependencies": {
        "emotion": "^9.2.6",
        "emotion-server": "^9.2.6",
        "gatsby": "next",
        "gatsby-plugin-emotion": "^1.1.17",
        "gatsby-plugin-typography": "^1.7.19",
        "gatsby-source-filesystem": "^1.5.39",
        "gatsby-transformer-remark": "^1.7.44",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-emotion": "^9.2.6",
        "react-typography": "^0.16.13",
        "typography": "^0.16.17"
    },
    "devDependencies": {
        "husky": "^1.0.0-rc.13",
        "lint-staged": "^7.2.0",
        "prettier": "1.14.0"
    },
    "prettier": {
        "tabWidth": 4,
        "semi": false
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
